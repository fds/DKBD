<input type="button" id="test" value="点击上传" />
<script type="text/javascript">
    var content = "data:image/jpeg;base64,";
    $("#test").click(function () {
        $.ajax({
            url: "/User/UploadFaceImg?" + accessTokenParm,
            type: "post",
            data: { content: content },
            success: function (data) {
                if (data.code == 0) {
                    document.write("上传成功,ULR:" + data.data);
                } else {
                    alert("上传失败,错误消息:" + data.message);
                }
            }
        });
    });
</script>